{
  "Class": {
    "GenerateDtoAttribute": "GenerateDto",
    "GenerateOptionalAttribute": "GenerateOptional",
    "Namespace": "WSM.Demo.Models.Dto",
    "GenerateEmptyConstructor": true,
    "GenerateFieldsConstructor": true,
    "MakePrivateAndPublicFields": true,
    "FieldNameFormat": "{lowertxt}",
    "PrivateFieldFormat": "{lowertxt}",
    "PublicFieldFormat": "{txt}",
    "MakeWholeNamespace": true,
    "NullableSupport": true,
    "OneGeneratorFile": false
  },
  "Client": {
    "MethodFormat": "{methodname}_Invoke",
    "ServiceFormat": "{controllerName}Client",
    "InterfaceFormat": "I{controllerName}Client",
    "ControllersPath": "..\\WSM.SourceGenerator.Demo.WebApi\\Controllers\\"
  }
}
